<body>
    <script>
        var x; //선언
        x = 10; //할당
        //선언+할당 = 초기화
        console.log(x);

        var x = 10, y = 11;

        console.log(x + y);
        var z = x + y;
        console.log(z)
    </script>

    <script>
        //let
        var a = 10;
        console.log(a);
        var a = 20;
        console.log(a);

        let b = 30;
        console.log(b);
        // let b = 40;
        // console.log(b);

        //const
        const c = 50;
        console.log(c)
        c = 60;

    </script>
